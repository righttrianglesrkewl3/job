<script>

$(document).ready(function() {

 if (document.documentElement.clientWidth <= 800) {

   $('#front_1').click(function() {

      var hght = $(this).css('height');
      if (hght == '26px') {
         $(this).css('height',550);
         $('#trend_arrow').attr('src', 'images/arrow-up-white.png');
      } else {
         $(this).css('height',26);
         $('#trend_arrow').attr('src', 'images/arrow-down-white.png');
      }
   });

